type Query {
    subscriptions : [Subscription]
}

type Show {
    title: String
    releaseYear: Int
}

type Subscription {
    id : Int
    subType : String
    name : String
    regiId : Int
    startDate : String
    endDate : String
    products : [Product]
}

type Product {
    id : Int
    price: Float
    name : String
    source : String
    startDate : String
    endDate : String
}
